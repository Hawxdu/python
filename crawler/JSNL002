    <script type="text/javascript">
        var flyOuts = new Object();
        flyOuts[0] = "/manuals";
        flyOuts[1] = "/config";

        // //variables created to handle the special characters returned to the URL after Eula/DSD scan
        var hashC = '#C';
        var hashTMC = '#TMC';
        var hashM = '#M';
        var browserlevelUrlUpdate = 'True';

        $(function () {

            if (navigator.appName == 'Microsoft Internet Explorer') {
                browserlevelUrlUpdate = 'False';
            }

            pSupInteractionHelper.ManageSection('JSNL002');

            // //Registering the click events on the flyouts.
            $("#manuals").bind('click', function (e) {

                if ($('#manualsLink').hasClass('icon-ui-collapse') && browserlevelUrlUpdate == 'True') {
                    UpdateUrl('/manuals', e);
                }
                else if(browserlevelUrlUpdate == 'True') {
                    UpdateUrl('', e);
                }
            });
            $("#config").bind('click', function (e) {

                if ($('#configLink').hasClass('icon-ui-collapse') && browserlevelUrlUpdate == 'True') {

                    UpdateUrl('/config', e);
                }
                else if(browserlevelUrlUpdate == 'True'){

                    UpdateUrl('', e);
                }
            });

            OpenFlyOut();
            function UpdateUrl(FlyOutText, e) {

                var $href = $(location).attr('href');
                $href = $href.replace(hashC, '')
                    .replace(hashTMC, '')
                    .replace(hashM, '');

                var urlEnd = $href.length;
                var sBiz = '';
                var sBizLen = $href.indexOf('?');


                // // Remove double slash if it's there

                if (urlEnd > 0) {
                    if ($href[urlEnd - 1] == "/") {
                        $href = $href.substr(0, urlEnd - 1);
                    }
                    if (sBizLen > 0) {
                        sBiz = $href.substr(sBizLen, urlEnd - sBizLen);
                        $href = $href.substr(0, sBizLen);
                    }
                }



                if ($href.indexOf('manuals') > 0) {
                    $href = $href.replace(flyOuts[0], '');

                }
                else if ($href.indexOf('config') > 0) {
                    $href = $href.replace(flyOuts[1], '');
                    $href = $href.replace(flyOuts[3], '');
                }

                if (sBiz != null) {
                    $href += FlyOutText + sBiz;
                }
                else {
                    $href += FlyOutText;
                }
                // // HTML5 Url push using History.js
                History.pushState({ url: $href }, document.title, $href);

                e.preventDefault();

            }
            // // Function to Open the flyout on Page Load
            function OpenFlyOut() {

                if ('' == 'manuals') {
                    pSupInteractionHelper.manualsClickHandler('poweredge-r420', 'PowerEdge R420');

                }
                else if ('' == 'config') {
                    pSupInteractionHelper.configurationClickHandler('JSNL002', 'M');

                }
            }
        }); 
</script> 

            </div>
        </div>
    </div>
</div>
