    <script type="text/javascript">
        var isDsdInstalled = false;
        var eulaDisplayed = false;
        /*var tokenExpires = '';
        var isAliveToken = '';
        var getServiceTagToken = '';*/

        var dsd = new $.eSupportClientApi();

        $(document).ready(function () {
            dsd.tokenExpires.setValue(tokenExpires);
            dsd.isAliveToken.setValue(isAliveToken);
            dsd.getClientSystemInfoToken.setValue(getServiceTagToken);
            dsd.findService(function () {
                isDsdInstalled = true;
            }, function (tagResponse) {
                isDsdInstalled = false;
            });

        });
    </script>
