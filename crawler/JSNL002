    $('.uif_mhCustomerLogo').one('load', function () {
        if (($('.uif_mhCustomerLogo').width() / $('.uif_mhCustomerLogo').height() > 3) && ($('.uif_mhCustomerLogo').width() > 150)) {
            $('.uif_mhCustomerLogo').height("auto").width(150);
