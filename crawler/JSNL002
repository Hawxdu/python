<script type="text/javascript" language="javascript">
    (function () {
        var onTabClick = function (e) {

            var $this = $(this),
                url = $this.data("tab-url"),
                tabName = $this.data("tab-name");

            // Middle click, cmd click, and ctrl click should open
            // links in a new tab as normal.
            if (e.which > 1 || e.metaKey || e.ctrlKey || e.shiftKey || e.altKey)
                return;
            e.preventDefault();
            //Update url

            History.pushState({ url: url, tabName: tabName, tabSelector: $this.attr("id") }, document.title, $this.find("a").attr("href"));
        },
            onStateChange = function (data, cleanUrl) {

                var tabName = data.tabName,
                    $tabDetails = $("#" + tabName),
                    $this = $("#" + data.tabSelector),
                    url = data.url;
                if (tabName == 'drivers') {
                    url = '/support/my-support/us/en/04/drivers/drivershome';
                }
                if (tabName == 'maintain') {
                    url = url + '/index/' + 'JSNL002';
                }
               
                if (tabName) {
                    $tabDetails.siblings(":not(#PageLoading)").hide();

                    if ($tabDetails.children().length === 0) {

//                        $("#PageLoading").removeClass("hideProgressTab");
//                        $("#PageLoading").addClass("showProgressTab");

                      $('#PageLoading').show().css('margin-top','50px');

                        $.ajax({
                            type: "GET",
                            url: url,
                            cache: false,
                            success: function (response) {
                                $tabDetails.html(response);

//                                $("#PageLoading").addClass("hideProgressTab");
//                                $("#PageLoading").removeClass("showProgressTab");

                                $('#PageLoading').hide().css('margin-top', '0px'); ;
                            },
                            error: function (response) {
//                                $("#PageLoading").addClass("hideProgressTab");
//                                $("#PageLoading").removeClass("showProgressTab");

                                $('#PageLoading').hide().css('margin-top', '0px'); ;

                                var json = jQuery.parseJSON(response.responseText);
                                $tabDetails.html(json.message);
                            }
                        });
                    }
                    updateOmiture(tabName, cleanUrl);
                    $tabDetails.show();

                    //setActiveArrow
                    $this.parent().find(".leftArrow").removeClass("leftArrow");
                    $this.children("div[id^='arw-']").addClass("leftArrow");

                    //SetActiveTab
                    $this.siblings(".active").removeClass("active");
                    $this.addClass('active');
                }
            },
        loadedUrl = History.getLocationHref(),

        tabOnLoad = 'maintain';

        seoTab = '';

        function updateOmiture(tabOn, cleanUrl) {
            /*Omniture code for the requirement-292502*/
            var prop13 = "esupport-mysupport|my-support|_index|tabs|";
            switch (tabOn) {
                case "get-started":
                    prop13 += "get_started";
                    break;
                case "maintain":
                    prop13 += "maintain";
                    break;
                case "diagnose":
                    prop13 += "diagnose_an_issue";
                    break;
                case "research":
                    prop13 += "research_a_topic";
                    break;
                case "drivers":
                    prop13 += "get_drivers";
                    break;
                case "upgrade":
                    prop13 += "upgrade";
                    break;
                default:
                    break;
            }

            myOmnitureScript(prop13, tabOn, cleanUrl);

        }

        History.Adapter.bind(window, 'statechange', function () { // Note: We are using statechange instead of popstate
            var state = History.getState(); // Note: We are using History.getState() instead of event.state

            if (state.data && state.data.url) {
                onStateChange(state.data, state.cleanUrl);
            } else {
                if (state.cleanUrl !== loadedUrl) {
                    window.location.href = state.cleanUrl;
                }
                onStateChange({ tabName: tabOnLoad, tabSelector: "tab-" + tabOnLoad, url: $("#tab-" + tabOnLoad).data("tab-url") }, state.cleanUrl);
            }
        });
        $(document).ready(function () {

            $("#tabs > li").click(onTabClick);

            // HF - Fixed 'Refresh' not working on HTML 4 browsers
            if (!(History.emulated.pushState && History.getHash())) {
                //Initalize the selected tab (we have to do this because the server can't do child webparts)
                //Ajax load is not required in case of direct url. seoTab will be empty for regular user workflow.
                if (seoTab == '') {
                    onStateChange({ tabName: tabOnLoad, tabSelector: "tab-" + tabOnLoad, url: $("#tab-" + tabOnLoad).data("tab-url") });
                }
                $("#" + tabOnLoad).show();
            }

            // show warranty details popup if parameter is passed through query string.

            var $warrantyModal = $('#warrantyModal');
            if ($warrantyModal.length > 0) {
                var query_string = location.search.replace(/\?|\//g, '');
                query_string = query_string.replace(/\?/gi, /\&/);
                var parms = query_string.split('&');

                for (var i = 0; i < parms.length; i++) {
                    var pos = parms[i].indexOf('=');
                    if (pos > 0) {
                        var key = parms[i].substring(0, pos);
                        var val = parms[i].substring(pos + 1);
                        if (key == 'show_warranty_modal' && val == 'true') {
                        if($('#warrantyModal') != null)
                            $('#warrantyModal').modal('show');
                        }
                    }
                }
                $('#warrantyModal').bind('shown', function () {
                    history.pushState({}, '', "?show_warranty_modal=true");
                    myOmnitureScript("us|en|bsd|04|esupport-mysupport|my-support|modal-warranty", "Warranty", state.cleanUrl);
                });
            }

        });


    })();

</script>

