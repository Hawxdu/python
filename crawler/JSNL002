<script>
    var currentPage = 'PS';
    $(document).ready(function () {
        $('#btnClose').bind('click', function () {
            if (eulaDisplayed == true) {
                eulaHelper.handleOldUserSelection();
            }
            $('#NotificationModal').modal('hide');
            return false;
        });

        $('#btnOld').bind('click', function () {
            if (eulaDisplayed == true || currentPage == 'MSEHome') {
                eulaHelper.handleOldUserSelection();
            }
            $('#NotificationModal').modal('hide');
            return false;
        });

        $('#btnNew').bind('click', function () {
            eulaHelper.handleNewUserSelection();

            $("#NotificationModal").hide();
            $('#NotificationModal').modal('hide');
        });
    }); 
</script>


