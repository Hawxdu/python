<script type="text/javascript">
   
   function rewriteLoginLink() {
        var loginLink = $("#loginLink");
       
        if ($(loginLink).length > 0) {
            $(loginLink).attr("href", "#");
            $(loginLink).click(function (e) {
                window.location = "https://www.dell.com/identity/v2/discovery?wa=wsignin1.0&wtrealm=http%3A%2F%2Fwww.dell.com&wreply=http%3A%2F%2Fwww.dell.com%2Fsupport%2Fmy-support%2Fen%2Fzh%2Fcnbsd1%2Fproduct-support%2Fservicetag%2FJSNL002%3Fs%3DBSD&whr=urn:transaction";
                e.preventDefault();
                
            });
        }
    }
    try {
        Dell.SupportMasthead.CountrySelector.keypress = function () { };
    }
    catch (ex) { }

       $(document).ready(function () {
        rewriteLoginLink();
    });
</script>
